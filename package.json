{
  "name": "demo-react-application",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-scripts-ts": "3.1.0",
    "react-text-mask": "^5.4.3",
    "react-toastify": "4.5.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --usePolling",
    "start-ts": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-ts",
    "build-ts": "react-scripts-ts build",
    "build": "npm-run-all build-css build-ts",
    "lint": "tslint -c tslint.json -p tsconfig.json ./src/**/*.{ts,tsx}",
    "test": "react-scripts-ts test --env=jsdom",
    "test-with-coverage": "react-scripts-ts test --env=jsdom --coverage",
    "test-without-watch": "set CI=true && react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/react-text-mask": "^5.4.3",
    "@types/react-toastify": "^4.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.7.0",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "typescript": "^3.2.4"
  }
}
